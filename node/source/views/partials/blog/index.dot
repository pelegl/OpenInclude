<div class="container blog">
    <h1>Blog</h1>
    {{~ it.posts :post }}
    <div class="post well well-small">
        <h1><a href="/blog/view/{{= post._id }}">{{= post.title }}</a> <small>{{= it.moment(post.date).format("MM/DD/YYYY") }}</small></h1>
        <div class="content">
            {{= post.htmlContent }}
        </div>
    </div>
    {{~}}
</div>