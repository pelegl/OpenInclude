<div class='row-fluid'>
  <div class='span12'>
      <h3>Users with stripe added</h3>
      <table class="table table-striped table-bordered userList">
          <thead>
            <tr>
                <th>User</th><th>Actions</th>
            </tr>
          </thead>
          <tbody>
            {{~it.users :user}}
            <tr>
                <td>
                    <a href='https://github.com/{{= user.github_username }}' target="_blank" >
                        <img src="http://www.gravatar.com/avatar/{{= user.github_avatar_url }}?s=40" class='avatar'  />
                        {{! user.github_username }}
                    </a>
                </td>
                <td>
                    <a href='issue_bill/{{= user.github_id }}' class='btn btn-success btn-mini'>Issue Bill</a>
                </td>
            </tr>
            {{~}}
          </tbody>
      </table>
  </div>
</div>
